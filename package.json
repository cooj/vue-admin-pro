{
    "name": "vue-admin",
    "type": "module",
    "version": "0.0.1",
    "description": "vue3 vite admin",
    "author": "cooj",
    "license": "MIT",
    "keywords": [
        "vue",
        "vue3",
        "element-plus"
    ],
    "engines": {
        "node": ">=16.0.0",
        "npm": ">= 7.0.0"
    },
    "scripts": {
        "dev": "vite --force",
        "build": "vite build",
        "lint": "eslint . --fix",
        "commit": "git-cz",
        "prepare": "husky install",
        "preinstall": "npx only-allow pnpm"
    },
    "dependencies": {
        "@element-plus/icons-vue": "^2.1.0",
        "@vueuse/core": "^10.5.0",
        "axios": "^1.3.5",
        "echarts": "^5.4.2",
        "element-plus": "^2.3.3",
        "js-cookie": "^3.0.1",
        "mitt": "^3.0.0",
        "nprogress": "^0.2.0",
        "pinia": "^2.0.34",
        "qrcodejs2-fixes": "^0.0.2",
        "qs": "^6.11.1",
        "sortablejs": "^1.15.0",
        "vue": "^3.2.47",
        "vue-clipboard3": "^2.0.0",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@antfu/eslint-config": "1.0.0-beta.27",
        "@iconify-json/ep": "^1.1.12",
        "@iconify-json/ic": "^1.1.14",
        "@types/node": "^18.15.11",
        "@types/nprogress": "^0.2.0",
        "@unocss/eslint-config": "^0.56.5",
        "@vitejs/plugin-vue": "^4.1.0",
        "@vue/compiler-sfc": "^3.2.47",
        "commitizen": "^4.3.0",
        "cz-git": "^1.7.1",
        "eslint": "^8.38.0",
        "husky": "^8.0.3",
        "lint-staged": "^15.0.2",
        "sass": "^1.61.0",
        "typescript": "^5.0.4",
        "unocss": "^0.56.5",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.2.1",
        "vite-plugin-cdn-import": "^0.3.5",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-vue-devtools": "1.0.0-rc.5"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ],
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
